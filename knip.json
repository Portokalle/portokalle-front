{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx,js,jsx}",
    "src/pages/**/*.{ts,tsx,js,jsx}",
    "src/components/**/*.{ts,tsx,js,jsx}",
    "pages/**/*.{ts,tsx,js,jsx}"
  ],
  "project": "**/*.{ts,tsx,js,jsx}",
  "ignore": [
    "**/node_modules/**",
    "**/.next/**",
    "**/dist/**",
    "**/build/**",
    "trash/**",
    "**/*.d.ts"
  ],
  "ignoreDependencies": [
    "eslint-*",
    "prettier"
  ]
}
